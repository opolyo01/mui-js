	<body>
		<div id="mui-views">
			<div id="root-view">
				<ul>
					<li class="mui-list-item mui-title mui-selectable" section-id="overview">Overview</li>
					<li class="mui-list-item mui-title mui-selectable" section-id="basics">The Basics</li>
					<li class="mui-list-item mui-title mui-selectable" section-id="libs">Libraries</li>
					<li class="mui-list-item mui-title mui-selectable" section-id="controls">Controls</li>
					<li class="mui-list-item mui-title mui-selectable" section-id="transitions">Transitions</li>
					<li class="mui-list-item mui-title mui-selectable" section-id="utils">Utilities</li>
					<li class="mui-list-item mui-title mui-selectable" section-id="webapps">Web Apps</li>
				</ul>
			</div>
			<div id="section-view">
				<div class="section" section-id="overview">
					<ul>
						<li class="mui-list-item mui-title mui-selectable" content-id="why">Why?</li>
					</ul>
				</div>
				<div class="section" section-id="basics">
					<ul>
						<li class="mui-list-item mui-title mui-selectable" content-id="code">Getting the Code</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="muiobject">The mui object</li>															
					</ul>
				</div>
				<div class="section" section-id="libs">
					<ul>
						<li class="mui-list-item mui-title mui-selectable" content-id="dom">DOM</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="event">Event</li>			
						<li class="mui-list-item mui-title mui-selectable" content-id="anim">Animation</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="io">Ajax</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="oop">OOP</li>
					</ul>
				</div>
				<div class="section" section-id="controls">
					<ul>
						<li class="mui-list-item mui-title mui-selectable" content-id="actionsheet">ActionSheet</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="button">Button</li>			
						<li class="mui-list-item mui-title mui-selectable" content-id="navbar">NavigationBar</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="pager">Pager</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="scrollview">ScrollView</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="searchbox">SearchBox</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="tabview">TabView</li>														
					</ul>
				</div>
				<div class="section" section-id="transitions">
					<ul>
						<li class="mui-list-item mui-title mui-selectable" content-id="overview">Overview</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="usage">Usage</li>			
					</ul>
				</div>
				<div class="section" section-id="utils">
					<ul>
						<li class="mui-list-item mui-title mui-selectable" content-id="datasource">DataSource</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="geo">Geo</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="navigator">Navigator</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="storage">Storage</li>
					</ul>
				</div>
				<div class="section" section-id="webapps">
					<ul>
						<li class="mui-list-item mui-title mui-selectable" content-id="overview">Overview</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="appcontroller">The Application Controller</li>			
						<li class="mui-list-item mui-title mui-selectable" content-id="viewcontrollers">View Controllers</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="navcontrollers">Navigation Controllers</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="events">Responding to Events</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="persistence">Persistence</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="navbar">Configuring the Navigation Bar</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="modal">Modal View Controllers</li>
						<li class="mui-list-item mui-title mui-selectable" content-id="splitviews">Split Views</li>															
					</ul>
				</div>
			</div>

			<div id="detail-view">
				<div id="detail-content"></div>
			</div>
			
		</div>
		
