		<title>mui.ScrollView y-axis</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
		
		<link rel="stylesheet" href="../../mui/assets/reset.css" type="text/css" media="screen" title="no title" charset="utf-8">
		<link rel="stylesheet" href="../../scroll-view/assets/skins/default/scroll-view-skin.css" type="text/css" media="screen" title="no title" charset="utf-8">
		<style type="text/css" media="screen">
		#main { width: 100%; position: absolute; top: 0px; left: 0; bottom: 0px; right: 0; height: 100%; }
		#images {
			white-space: nowrap;
		}
		#images li {
			display: inline-block;
		}
		</style>
		
		<script src="../../mui/js/mui.js" type="text/javascript" charset="utf-8"></script>
		<script src="../../scroll-view/js/ScrollView.js" type="text/javascript" charset="utf-8"></script>
		
		<script type="text/javascript" charset="utf-8">		
		mui.on(window, 'load', function() {
			document.body.style.minHeight = window.innerHeight+'px';
			document.body.style.overflow = 'hidden';
			
			var scrollView = new mui.ScrollView({
				element: '#the-element',
				size: '100%'
			});
			
			var imageScrollView = new mui.ScrollView({
				element: '#images',
				axis: 'x',
				size: '100%',
				showsHorizontalScrollIndicator: false
			});
		});
		</script>
	</head>
	
